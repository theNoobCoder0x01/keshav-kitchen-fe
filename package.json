{
  "name": "keshav-kitchen-fe",
  "version": "0.1.0",
  "private": true,
  "description": "Keshav Kitchen Frontend",
  "scripts": {
    "build": "next build",
    "db:generate": "npx dotenv -e .env.local -- npx prisma generate",
    "db:migrate": "npx dotenv -e .env.local -- npx prisma migrate dev",
    "db:migrate:prod": "npx dotenv -e .env.local -- npx prisma migrate deploy",
    "db:push": "npx dotenv -e .env.local -- npx prisma db push",
    "db:reset": "npx dotenv -e .env.local -- npx prisma migrate reset --force",
    "db:seed": "npx dotenv -e .env.local -- node prisma/seed.js",
    "db:studio": "npx dotenv -e .env.local -- npx prisma studio",
    "deploy:setup": "npm install --production && npm run setup",
    "dev": "next dev",
    "lint": "next lint",
    "setup": "npm install && npm run db:generate && npm run db:push && npm run db:seed",
    "start": "next start",
    "prettier": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
