{
  "name": "keshav-kitchen-fe",
  "version": "0.1.0",
  "private": true,
  "description": "Keshav Kitchen Frontend",
  "scripts": {
    "dev": "next dev",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "build": "next build",
    "start": "next start",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "db:push": "prisma db push",
    "db:mark1": "prisma migrate resolve --applied 20250817211740_add_ingredient_groups --schema=prisma/schema.prisma",
    "db:mark2": "prisma migrate resolve --applied 20250830233050_menu_component --schema=prisma/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy",
    "setup:dev": "npm run db:generate && npm run db:migrate:prod && npm run db:seed",
    "setup:prod": "npm run db:generate && npm run db:migrate:prod && npm run db:seed",
    "deploy:dev": "docker compose -f docker-compose.base.yml -f docker-compose.dev.yml up -d --build",
    "deploy:prod": "docker compose -f docker-compose.base.yml -f docker-compose.prod.yml up -d --build"
  },
