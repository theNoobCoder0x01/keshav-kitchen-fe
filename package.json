{
  "name": "kitchen-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "db:seed": "dotenv -e .env.local -- node prisma/seed.js",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "dev": "next dev",
    "lint": "next lint",
    "setup": "npm install && npm run db:generate && npm run db:push && npm run db:seed",
    "start": "next start"
  },
  "dependencies": {
    "@auth/core": "latest",
    "@neondatabase/serverless": "^0.9.0",
    "@prisma/client": "latest",
    "bcryptjs": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "latest",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "next-auth": "latest",
    "nodemailer": "latest",
    "path": "latest",
    "prisma": "^5.7.0",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- node prisma/seed.js"
  }
}